//This method is used to get product list start
Public void ChangeProductlist(){
     selectProdList =getproduct();
    CaseReasonTypeList=getcasereason();
 }
  Public list<SelectOption> getproduct(){  
       list<SelectOption> options= new List<selectOption>();
       list <product2> prolist=new list <product2>();
       if(ProductFamily!=''){
           system.debug('product family'+ProductFamily);
           for(Product2 p2:[select id ,Name,Family,Active__c from Product2 where Family =:ProductFamily and Active__c != False]){
               options.add(new SelectOption(p2.name, p2.name));
           }
       }
      return options;
   }
//This method is used to get product list start
//This method is used to case reason dependent on product family start
   Public list<SelectOption> getcasereason()
   {
   case ca2 =new case();
    List<selectOption> CaseReasonType = new List<selectOption>();
              If(ProductFamily =='High Bright Signage Display')
              {
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Issue','Issue'));     
                return CaseReasonType;
                }
                else if(ProductFamily =='Videowall Display')
                {
                 List<selectOption> CaseReasonType1 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Issue','Issue'));  
                  CaseReasonType.add(new SelectOption('Calibration','Calibration'));   
                return CaseReasonType;
                }
                else if(ProductFamily =='Indoor LED Display')
                {
                List<selectOption> CaseReasonType2 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Project Support','Project Support'));  
                  CaseReasonType.add(new SelectOption('Issue','Issue'));   
                return CaseReasonType;
                }
                else if(ProductFamily=='Signage Display')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Issue','Issue'));  
                  CaseReasonType.add(new SelectOption('App Signing','App Signing'));   
                return CaseReasonType;
                }
                else if(ProductFamily=='ProTv')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Issue','Issue'));  
                  CaseReasonType.add(new SelectOption('CDB','CDB')); 
                  CaseReasonType.add(new SelectOption('SXP','SXP'));
                  CaseReasonType.add(new SelectOption('MyChoice','MyChoice'));  
                  CaseReasonType.add(new SelectOption('Vsecure','Vsecure'));   
                return CaseReasonType;
                }
                else if(ProductFamily=='LED')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                  CaseReasonType.add(new SelectOption('Project Support','Project Support'));  
                  CaseReasonType.add(new SelectOption('Issue','Issue')); 
                return CaseReasonType;
                }
                else if(ProductFamily=='Touch')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                  CaseReasonType.add(new SelectOption('CMND','CMND'));
                  CaseReasonType.add(new SelectOption('Inquiry','Inquiry'));
                 CaseReasonType.add(new SelectOption('Issue','Issue'));  
                  CaseReasonType.add(new SelectOption('App Signing','App Signing'));   
                return CaseReasonType;
                }
                else if(ProductFamily=='OPS')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                   CaseReasonType.add(new SelectOption('CRD50','CRD50'));
                return CaseReasonType;
                }
                 else if(ProductFamily=='Accesories')
                {
                List<selectOption> CaseReasonType3 = new List<selectOption>();
                  CaseReasonType.add(new SelectOption('-None-','-None-'));
                   CaseReasonType.add(new SelectOption('Remote Control','Remote Control'));
                   CaseReasonType.add(new SelectOption('CRD41','CRD41'));
                return CaseReasonType;
                }
                return null;
   }
//This method is used to case reason dependent on product family end
//This method is used to create case reason dependent on case form functionality start 
      public Void casereasonform()
     {
      if(CaseReasonType== 'CMND' && ProductFamily=='ProTv')
     {
     showinteger=13;
     }
    else if(CaseReasonType== 'Issue' && ProductFamily=='ProTv')
     {
     showinteger=14;
     }
     else if(CaseReasonType== 'CMND' && ProductFamily !='ProTv')
     {
     showinteger=1;
     }
     else if(CaseReasonType== 'Inquiry')
     {
    showinteger=2;
     }
     else if(CaseReasonType== 'Issue')
     {
    showinteger=3;
     }
     else if(CaseReasonType== 'Calibration')
     {
    showinteger=4;
     }
     else if(CaseReasonType== 'Project Support')
     {
    showinteger=5;
     }
     else if(CaseReasonType== 'App Signing')
     {
    showinteger=6;
     }
     else if(CaseReasonType== 'CDB')
     {
    showinteger=7;
     }
     else if(CaseReasonType== 'SXP')
     {
    showinteger=8;
     }
      else if(CaseReasonType== 'MyChoice')
     {
    showinteger=9;
     }
     else if(CaseReasonType== 'Vsecure')
     {
     showinteger=10;
     }
     else if(CaseReasonType== 'CRD50')
     {
     showinteger=11;
     }
     else if(CaseReasonType== 'Remote Control')
     {
     showinteger=12;
     }
     else if(CaseReasonType== 'CRD41')
     {
     showinteger=12;
     }
     
     }  
   
//This method is used to create case reason dependent on case form functionality end 

