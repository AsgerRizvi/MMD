 //This method is used check whether email is duplicates or not start
 public void DuplicateEmailMethod(){
      list<Case> caList =[select Id,Model_Number__c ,Serial_Number__c ,Bill_Number__c,Purchase_Date__c,ContactId,ContactEmail ,Complaint_Type__c from Case where  ContactEmail=:hemail];
     if(caList.size()>0){
            errormessage='You already Have existing Account from this Email.Please login to your Account to register the case';
          }else{
              errormessage='';
          }
 }
 public void ShowHide(){
      system.debug('hlstname===='+hlstname);
     if(TagIndex==1){
         TagIndex=2;
         system.debug('TagIndex1---'+TagIndex);
     }else  if(TagIndex==2){
         TagIndex=1;
         system.debug('TagIndex2---'+TagIndex);
     }
 }

