// It is for the attachment used only start 
   public static  PageReference  getAttachment()
       {
       system.debug('contact id =============='+contId);       
       system.debug('filebody====== '+filebody) ; 
       list<case>   casee   = [select id from case order by CreatedDate DESC limit 1];
       system.debug('size---------'+casee.size());
       if(casee.size()>0 && filebody !=null && fileName != '' ){                                                      
           Attachment myAttachment = new Attachment();
           myAttachment.Body = filebody ;  
           system.debug('filebody======'+myAttachment.Body) ;                         
           myAttachment.Name = fileName ;
           myAttachment.ParentId = casee[0].id;          
           system.debug('attachment called');
           insert myAttachment;
            system.debug('attachment inserted'+myAttachment.id);      
       }
      
     return null;                   
                     
     }
  // It is for the attachment used only end
  
  
